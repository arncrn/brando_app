{
  "name": "brando_app",
  "version": "1.0.0",
  "description": "",
  "main": "app.js",
  "scripts": {
    "heroku-postbuild": "npm install --production",
    "start": "NODE_ENV=production node index.js",
    "dev": "NODE_ENV=development nodemon index.js",
    "backup": "pg_dump -d brandomania3 --inserts > backup.sql",
    "backup:heroku": "dropdb brandomania_backup && heroku pg:pull DATABASE_URL brandomania_backup --app pacific-everglades-84945",
    "refresh-db": "heroku pg:backups:capture --app pacific-everglades-84945 && heroku pg:backups:download --app pacific-everglades-84945 && dropdb brandomania_backup && createdb brandomania_backup && pg_restore --verbose --no-owner --no-comments --dbname=brandomania_backup latest.dump",
    "sql-file:heroku": "pg_dump -d brandomania --inserts > heroku.sql",
    "test": "NODE_ENV=test jest --verbose --runInBand",
    "build": "webpack"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@aws-sdk/client-cognito-identity": "^3.25.0",
    "@aws-sdk/client-s3": "^3.17.0",
    "@aws-sdk/client-sqs": "^3.25.0",
    "@aws-sdk/credential-provider-cognito-identity": "^3.25.0",
    "aws-sdk": "2.x",
    "axios": "^0.22.0",
    "babel-cli": "^6.26.0",
    "babel-preset-react-app": "^3.1.2",
    "bcrypt": "^5.0.1",
    "bull": "^3.28.1",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "ejs": "2.x",
    "express": "^4.17.1",
    "express-handlebars": "^5.2.0",
    "express-session": "^1.17.1",
    "get-stream": "^6.0.1",
    "jimp": "^0.16.1",
    "jsonwebtoken": "^9.0.2",
    "multer": "^1.4.2",
    "pg": "^8.5.1",
    "readable-stream": "^3.6.0"
  },
  "devDependencies": {
    "jest": "^27.0.6",
    "nodemon": "^2.0.6",
    "path-browserify": "^1.0.1",
    "supertest": "^6.1.3",
    "webpack": "^5.49.0",
    "webpack-cli": "^4.7.2"
  },
  "engines": {
    "node": "12.19.0",
    "npm": "6.14.8"
  },
  "jest": {
    "testEnvironment": "node"
  }
}
